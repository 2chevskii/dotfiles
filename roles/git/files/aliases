[alias]
  list-alias = config --get-regexp alias
  s = status
  lo = log --oneline
  la = log --all
  lg = log --graph
  lgo = log --graph --oneline
  lga = log --graph --all
  lgao = log --graph --all --oneline
  last = log --stat -1
  cm = commit
  remotes = remote -v
  del = branch -d
  delf = branch -D
  pushdel = push --delete
  pushf = push --force
  c = checkout
  br = branch -l
  bra = branch -la
  b = branch --format='%(HEAD) %(color:yellow)%(refname:short)%(color:reset) - %(contents:subject) %(color:green)(%(committerdate:relative)) [%(authorname)]' --sort=-committerdate
  bname = rev-parse --abbrev-ref HEAD
  sw = switch
  st = stash push
  stp = stash pop
  rs = reset
  undo = reset HEAD~1 --mixed
  f = fetch
  m = merge
  done = push origin HEAD
